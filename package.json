{
  "name": "react-color-palette-generator",
  "homepage": "https://jepeteo.github.io/react-color-palette-generator",
  "author": "Theodoros Mentis",
  "private": true,
  "version": "0.5.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "build": "vite build",
    "lint": "eslint src --report-unused-disable-directives",
    "format": "prettier --write \"src/**/*.{js,jsx,css,md}\"",
    "preview": "vite preview",
    "postinstall": "node -e \"const p=process.platform;const a=process.arch;const pkg=p==='win32'?'@rollup/rollup-win32-'+a+'-msvc':p==='darwin'?'@rollup/rollup-darwin-'+a:'@rollup/rollup-linux-'+a+'-gnu';require('child_process').execSync('npm install '+pkg+' --no-save',{stdio:'inherit'})\""
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.7",
    "chroma-js": "^2.4.2",
    "extract-colors": "^4.0.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-redux": "^9.1.2",
    "react-tooltip": "^5.28.0",
    "redux": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "gh-pages": "^6.3.0",
    "globals": "^15.13.0",
    "postcss": "^8.4.40",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.7",
    "vite": "^5.4.20"
  },
  "overrides": {
    "esbuild": "^0.25.0"
  }
}
